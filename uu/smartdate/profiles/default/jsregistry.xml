<?xml version="1.0"?>
<object name="portal_javascripts">
  <!-- jquery tools dateinput, from plone.app.jquerytools, which does not
       enable this resource by default, so we do it here -->

  <javascript
    cacheable="True"
    compression="none"
    cookable="True"
    enabled="True"
    id="++resource++plone.app.jquerytools.dateinput.js"
    inline="False"
    expression=""
    insert-after="++resource++plone.app.jquerytools.js"
    />

  <javascript
    cacheable="False"
    compression="none"
    cookable="False"
    enabled="True"
    id="@@date.js"
    inline="False"
    expression=""
    insert-after="++resource++plone.app.jquerytools.dateinput.js"
    />

  <javascript
    cacheable="True"
    compression="safe"
    cookable="True"
    enabled="True"
    id="++resource++uu.smartdate-scripts/smartdate.js"
    inline="False"
    expression=""
    insert-after="@@date.js"
    />

  <javascript
    cacheable="True"
    compression="safe"
    cookable="True"
    enabled="True"
    id="++resource++uu.smartdate-scripts/monkeypatch_datagrid.js"
    inline="False"
    expression=""
    insert-after="++resource++collective.z3cform.datagridfield/datagridfield.js"
    />

</object>
